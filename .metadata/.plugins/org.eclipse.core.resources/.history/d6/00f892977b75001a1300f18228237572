
public class Test2 {

	public static void main(String[] args) {
		//switch ~ case
		//정수식 : 정수형변수, 값, 문자형(char), 문자열(한글/영어)
		/*switch (정수식) {
		case 값1:
			실행문;
			[break;]
		case 값2:
			실행문;
			[break;]
		default:
			실행문;
			break;
		}
		*/
		switch (10) {
		case 10:
			System.out.println("10");
			break;
		case 9:
			System.out.println("9");
			break;
		case 8:
			System.out.println("8");
			break;
		default:
			System.out.println("해당사항 없음");
			break;
		}
	}

}
