
public class Test1 {

	public static void main(String[] args) {
		// 2차원 배열(다차원 배열) : 행렬표시 가능(표)
		// 반복문의 개수 증가 : 이중 for문
		// 1. new 연산자
		// 2. 직접 초기화
		
		// 5 x 3 행렬 표현하기 위한 2차원 배열
		int[][] arr = new int[5][3];
		
		arr[0][0] = 10; arr[0][1] = 50; arr[0][2] = 60;
		arr[1][0] = 12; arr[1][1] = 42; arr[1][2] = 64;
		arr[2][0] = 56; arr[1][1] = 35; arr[2][2] = 73;
		arr[3][0] = 78; arr[1][1] = 16; arr[3][2] = 23;
		arr[4][0] = 51; arr[1][1] = 48; arr[4][2] = 80;
		
		System.out.println(arr.length);
	}

}
