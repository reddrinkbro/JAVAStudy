package homework1;

public class Gugudan {
	public static void main(String[] args)
	{
		for (int i = 2; i <= 9; i++) {
			System.out.print(i + "´Ü\t\t");
		}
		for (int i = 1; i <= 9; i++) {
			for (int j = 2; j <= 9; j++) {
				System.out.print(j + " x "+ i + " = " + i * j + "\t");
			}
			System.out.println();
		}
	}
}
