
public class Test02 {
	public static void main(String[] args)
	{
		//¿¬»êÀÚ
		
		float f1 = 3.123f;
		float f2 = 2.111f;
		System.out.println("f1 == f2 : " + (f1 == f2));
		System.out.println("f1 != f2 : " + (f1 != f2));
		System.out.println("f1 < f2 : " + (f1 < f2));
		System.out.println("f1 > f2 : " + (f1 > f2));
		System.out.println("f1 <= f2 : " + (f1 <= f2));
		System.out.println("f1 >= f2 : " + (f1 >= f2));
	}
}
