
public class Test2 {

	public static void main(String[] args) {
		int[] stars={1,6,7,9,5,3,4,6,2,8};
		
		for (int i = 0; i < stars.length; i++) {
			System.out.print("¹è¿­ °ª : " + stars[i] + " ");
			for (int j = 0; j < stars[i]; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
	}

}
