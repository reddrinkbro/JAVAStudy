
public class Test2 {

	public static void main(String[] args) {
		//숫자 판독 프로그램 생성
		String str = "12g45";
		char ch = ' ';
		boolean isNumber = false;
		for (int i = 0; i < str.length(); i++) {
			if((str.charAt(i) >= 'a' && str.charAt(i) <= 'z')||(str.charAt(i) >= 'A' && str.charAt(i) <= 'Z')){
				System.out.println("숫자가 아니다");
				break;
			}
		}
	}

}
