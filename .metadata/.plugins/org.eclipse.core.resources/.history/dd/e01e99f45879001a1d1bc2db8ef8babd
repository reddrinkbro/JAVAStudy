
public class Test1 {

	public static void main(String[] args) {
		//while()문
		/*while(조건식){
			반복할 코드
			증감식
		}*/
		
		//1 ~ 10까지 출력
		for(int i = 1 ; i <= 10; i++ )
		{
			System.out.print(i + " ");
		}
		System.out.println();
		int a = 1;
		while(a<=10){
			System.out.print(a++ + " ");
		}
		System.out.println("-----------------------------------------");
		int i = 1;
		while(i<=10){
			System.out.print(i + " ");
			i+=2;
		}
	}

}
