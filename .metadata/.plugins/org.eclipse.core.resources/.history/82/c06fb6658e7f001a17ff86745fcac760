
public class Test2 {

	public static void main(String[] args) {
		int[] stars={1,6,7,9,5,3,4,6,2,8};
		
		for (int i = 0; i < stars.length; i++) {
			System.out.print(stars[i] + " ");
			for (int j = 0; j < stars[i]; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
		int[] num = new int[3];
		int check = (int)(Math.random() * 10) + 1;
		for (int i = 0; i < num.length; i++) {
			int check2 = (int)(Math.random() * 10) + 1;
			if(check == 0) check = (int)(Math.random() * 10) + 1;
			
			num[i] = check;
		}
		
		
	}
}
