
public class Test2 {

	public static void main(String[] args) {
		//숫자 판독 프로그램 생성
		String str = "12g45";
		char ch = ' ';
		boolean isNumber = false;
		for (int i = 0; i < str.length(); i++) {
			ch = str.charAt(i);
			isNumber = true;
			if((ch >= 'a' && ch <= 'z')||(ch >= 'A' && ch <= 'Z')){
				System.out.println("숫자가 아니다");
				isNumber = false;
				break;
			}
		}
		if(isNumber) System.out.println("숫자 입니다");
	}

}
